------------------------------------------------------- LOCALPLAYER ----------------------------------------------------  

local LocalPlayer = Window:MakeTab({
     Name = "LocalPlayer",
     Icon = "rbxassetid://4483345998",
     PremiumOnly = false
})

LocalPlayer:AddButton({
     Name = "Immunity",
     Callback = function()
          game:GetService("ReplicatedStorage").RemoteEvents.MakeStealth:FireServer(9999999)
     end
})


LocalPlayer:AddButton({
     Name = "Instant heal",
     Callback = function()
          game:GetService("ReplicatedStorage").RemoteEvents.Cattery:FireServer()
          for i=1, 100 do 
               local args = {
                    [1] = "Cat"
               }

               game:GetService("ReplicatedStorage").RemoteEvents.Energy:FireServer(unpack(args))
          end
     end
})

LocalPlayer:AddButton({
     Name = "Code note",
     Callback = function()
          game.workspace.CodeNote.CFrame = char.HumanoidRootPart.CFrame
     end
})

