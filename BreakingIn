------------------------------------------------------- FUN ----------------------------------------------------                  


local Players = game:GetService("Players")

local char = Players.LocalPlayer.Character


local PlrTable = {}

for i, v in pairs(Players:GetPlayers()) do
     table.insert(PlrTable, v.Name)
end

local Fun = Window:MakeTab({
     Name = "Fun",
     Icon = "rbxassetid://4483345998",
     PremiumOnly = false
})



Fun:AddDropdown({
     Name = "Drown Someone",
     Default = "Plr Here",
     Options = PlrTable,
     Callback = function(Value)
          local args = {
               [1] = 1,
               [2] = Players[Value],
          }

          game:GetService("ReplicatedStorage").RemoteEvents.ToxicDrown:FireServer(unpack(args))
     end    
})

Fun:AddButton({
     Name = "Refresh Player Dropdown",
     Callback = function()
          oadstring(game:HttpGet(('https://raw.githubusercontent.com/lmaolmaoyaaa/BreakingIn/main/BreakingIn')))()
     end
})


Fun:AddButton({
     Name = "Drown everyone",
     Callback = function()
          for i , v in pairs(Players:GetPlayers()) do
               local args = {
                    [1] = 1,
                    [2] = v,
               }

               game:GetService("ReplicatedStorage").RemoteEvents.ToxicDrown:FireServer(unpack(args))
          end
     end    
})


Fun:AddButton({
     Name = "Drown others",
     Callback = function()
          for i , v in pairs(Players:GetPlayers()) do

               if v.Name ~= Players.LocalPlayer.Name then
                    local args = {
                         [1] = 1,
                         [2] = v,
                    }

                    game:GetService("ReplicatedStorage").RemoteEvents.ToxicDrown:FireServer(unpack(args))
               end
          end
     end    
})



